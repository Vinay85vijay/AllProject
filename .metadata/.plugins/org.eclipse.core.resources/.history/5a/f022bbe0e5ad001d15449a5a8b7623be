package com.hello;

public class test111 {

	public static void main(String[] args) {
		int a[]= {22,20,18,24};
		sort(a);
		for(int n:a) {
		System.out.println(n);
		}
	}
	public static void sort(int[] a) {
		int i=0; int j=a.length-1;
				while(i<j) {
			int temp=a[i];
			a[i]=a[j];
			a[j]=temp;
			i++;
			j--;
		}
		
	}
	

}
