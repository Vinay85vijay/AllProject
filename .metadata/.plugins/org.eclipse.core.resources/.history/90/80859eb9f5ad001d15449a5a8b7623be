package com.hello;

public class ShiftZero {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[]= { 1,2,3,0,0,0};
		int arr1[]= new int[arr.length];
		int j=0;
		for(int i=0;i<arr.length;i++) {
			if(arr[i]!=0) {
				arr1[j]=arr[i];
			}
		}
		for(int i=arr1.length-1;i>0;i--) {
			arr1[j]=arr1[i];
		}
		System.out.println(arr1[j]);
		

	}

}
