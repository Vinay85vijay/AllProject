package com.hello;

public class RightRot {

	public static void main(String[] args) {
       int arr[]= {1,2,3,4,5};
       int size=arr.length;
       int n=3;
       for(int i=0;i<n;i++) {
    	   
    	   int j,last=0;
    	   last=arr[size-1];
    	   for( j=size-1;j>0;j--) {
    		   arr[j]=arr[j-1];
    		   
    	   }
    	   arr[0]=last;
       }
       for(int i = 0;i<arr.length;i++) {
    	   System.out.println(arr[i]);
       }
	}

}
