package com.hello;

public class NextGreaterEvenNumber {

	public static void main(String[] args) {
		int arr[]= {2, 5, 6, 11, 9, 13, 14, 12};
		int element =9;
		for(int i=0;i<arr.length;i++) {
			if((arr[i]%2==0)) {
				if(arr[i]%2==0) {
					i++;
				}
				int j = arr[i];
				System.out.println(j);
			}
			
		}
		
		

	}

}
