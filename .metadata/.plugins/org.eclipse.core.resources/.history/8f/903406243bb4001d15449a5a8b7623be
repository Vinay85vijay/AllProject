package com.hello;

public class NextGreaterEvenNumber {
int arr[];
static int j;
	public static void main(String[] args) {
		int arr[]= {2, 5, 6, 11, 9, 13, 14, 12};
		int element =9;
		for(int i=0;i<arr.length;i++) {
			if(element<arr[i]) {
				arr[j++]=arr[i];
				System.out.println(j);
			}
			
		}
		
		

	}

}
